exLerMaiorMenor()
{
    inteiro n,i,d;
    real s;

    s = 1;
    d = 0;
    imprima(" Digite o valor de n: ");
    leia(n);

    para (i = 1; i <= n; i = i + 1)
    {
        s= (s + i)/(n - d);
        d = d + 1;
        imprima(s);
        imprima("\n");
    }

    imprima("S Ã© igual a: ");
    imprima(s);

}
